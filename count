num=$1
if [ $# -eq 0 ]
	then
	echo 인수가 없습니다
elif [ $# -eq 1 ]
	then
	case $num
	in
		1) echo Jan;;
		2) echo Feb;;
		3) echo Mar;;
		4) echo Apr;;
		5) echo May;;
		6) echo Jun;;
		7) echo Jul;;
		8) echo Aug;;
		9) echo Sep;;
		10) echo Oct;;
		11) echo Nov;;
		12) echo Dec;;
	esac
else
	echo 인수가 2개 이상입니다
fi